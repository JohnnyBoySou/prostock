{
  "expo": {
    "name": "ProStock",
    "slug": "ProStock",
    "version": "11.0.0",
    "scheme": "prostock",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#019866"
    },
    "ios": {
      "googleServicesFile": "./GoogleService-Info.plist",
      "supportsTablet": false,
      "requireFullScreen": true,
      "buildNumber": "11.0.0",
      "bundleIdentifier": "com.engd.prostock",
      "infoPlist": {
        "NSCameraUsageDescription": "Esse app usa a câmera para escanear documentos com Inteligência Artificial."
      }
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "package": "com.engd.prostock",
      "versionCode": 11,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#019866"
      },
      "blockedPermissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.READ_MEDIA_AUDIO",
        "android.permission.READ_MEDIA_VIDEO"
      ],
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Permita que o ProStock acesse sua câmera."
        }
      ],
      "expo-font",
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "DEFAULT"
        }
      ],
      [
        "onesignal-expo-plugin",
        {
          "mode": "production",
          "smallIcons": [
            "./assets/small_icon.png"
          ],
          "largeIcons": [
            "./assets/large_icon.png"
          ],
          "smallIconAccentColor": "#FF26BD"
        }
      ]
    ],
    "extra": {
      "oneSignalAppId": "a761f833-9b85-4811-ac5b-b5cd68313b34"
    }
  }
}